@{
    ViewBag.Title = "Details";
}
@{
    List<String> date = ViewData["MyProduct600"] as List<string>;
    List<double> position = new List<double>();
}


<style>
    #map {
        width: 100%;
        height: 400px;
        background-color: grey;
        overflow: visible;
    }
</style>

<div class="row">
    <br />
    <div class="col-md-3">
        <h3>Search Fitness Centers:</h3>
    </div>
    <div class="col-md-8">
        <div class="row">
            <br />
            <div class="col-md-3">
                <input type="text" value="" id="searchPlace">
            </div>
            <div class="col-md-5">
                <input type="button" value="Search" id="btn" />
            </div>
        </div>
    </div>
</div>
<script src="~/Scripts/FourSquare.js"></script>
<div class="row">
    <div class="col-md-8">
        <div id="map" onload="hello()"></div>
    </div>
    <div class="col-md-4">
        <div class="well">
            <div class="row">
                <div class="col-md-12">
                    <p>Users checked in at location:</p>
                </div>
                <div class="col-md-12">
                    <select style="min-width:100%;" id="users" name="user"></select>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-4">
        <div class="well">
            <p>Choose a location: <select style="min-width:90%;" id="year" name="year"></select></p>
        </div>
    </div>
    <div class="col-md-8">
        <div class="row">
            <br />
            <div class="col-md-12">
                <input type="button" value="Check in" onclick="CheckIn()" />
            </div>
            <br />
            <br />
            <div class="col-md-12">
                <input type="button" value="See users checked in at location" onclick="GetUsers()" />
            </div>
        </div>
    </div>
</div>

<script async defer
            src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDrvkfQB_Mfp7L9r1PKvU8AnTFmm9O8RlA&callback=initialize">
</script>
